[TimeSeriesCSV]
 Title = Create CSV data file.
 Abstract = Create CSV data file data provided through OPeNDAP streams.
 processVersion = 2
 storeSupported = true
 statusSupported = true
 serviceProvider = TimeSeriesCSV
 serviceType = Python
 <MetaData>
   title = Demo
 </MetaData>
  <DataInputs>
  [odurl]
   Title = OPeNDAP URL
   Abstract = The URL of OPeNDAP stream. Exactly one URL is required.
   minOccurs = 0
   maxOccurs = 1
   <MetaData>
    title = OPeNDAP URL
   </MetaData>
   <LiteralData>
    DataType = string
    <Default>
    </Default>
    <Supported>
    </Supported>
   </LiteralData>
  [varSNList]
   Title = varSNList
   Abstract = List of CF variable standard names
   minOccurs = 0
   maxOccurs = 999
   <MetaData>
    title = varSNList
   </MetaData>
   <LiteralData>
    DataType = string
    <Default>
    </Default>
    <Supported>
    </Supported>
   </LiteralData>
  [yvar]
   Title = yvar
   Abstract = The variable to plot on the y-axis. This is the OPeNDAP short_name of the variable.
   minOccurs = 0
   maxOccurs = 1
   <MetaData>
    title = yvar
   </MetaData>
   <LiteralData>
    DataType = string
    <Default>
    </Default>
    <Supported>
    </Supported>
   </LiteralData>
   [everyNth]
    Title = everyNth
    Abstract = This value is used to plot a subset of the data. Every "everyNth" point will be used. Set this to 1 to plot every data point.
    minOccurs = 0
    maxOccurs = 1
    <MetaData>
     title = everyNth
    </MetaData>
    <LiteralData>
     DataType = integer
     <Default>
     </Default>
     <Supported>
     </Supported>
    </LiteralData>

    [fileName]
     Title = fileName
     Abstract = The file name to store the plot in.
     minOccurs = 0
     maxOccurs = 1
     <MetaData>
      title = fileName
     </MetaData>
     <LiteralData>
      DataType = string
      <Default>
      </Default>
      <Supported>
      </Supported>
     </LiteralData>

     [fileFormat]
      Title = fileFormat
      Abstract = The output file format.
      minOccurs = 0
      maxOccurs = 1
      <MetaData>
       title = fileFormat
      </MetaData>
      <LiteralData>
       DataType = string
       <Default>
       </Default>
       <Supported>
       </Supported>
      </LiteralData>


 </DataInputs>
 <DataOutputs>
  [Result]
   Title = CSV data
   Abstract = The URL of the plot file.
   <MetaData>
    title = CSV data
   </MetaData>
   <LiteralData>
    DataType = string
    <Default>
    </Default>
    <Supported>
    </Supported>
   </LiteralData>


 </DataOutputs>


 <DataOutputs>
  [Debugtest]
   Title = CSV data
   Abstract = The URL of the plot file.
   <MetaData>
    title = CSV data
   </MetaData>
   <LiteralData>
    DataType = string
    <Default>
    </Default>
    <Supported>
    </Supported>
   </LiteralData>


 </DataOutputs>

 <DataOutputs>
  [CSVOutputFileURL]
   Title = CSV data
   Abstract = The URL of the CSV file.
   <MetaData>
    title = CSV data
   </MetaData>
   <LiteralData>
    DataType = string
    <Default>
    </Default>
    <Supported>
    </Supported>
   </LiteralData>


 </DataOutputs>
